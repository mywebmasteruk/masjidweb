          "_customHTML": "\n\n\n\n\n\n\n<section class=\"mbr-section form3 cid-raQ764dc9S\" id=\"form1-l\">\n\n    <div class=\"container\">\n        \n         <div class=\"row justify-content-center\">\n            <div class=\"form-1 col-md-10 col-lg-8\">\n                <h2 class=\"mbr-fonts-style mbr-section-title align-center display-2\">\n                    Contact Form</h2>\n                <h3 class=\"mbr-fonts-style mbr-section-subtitle align-center display-5\">You can also reach us directly via <a href=\"http://bit.ly/2GNgSIE\" target=\"_blank\"> <img src=\"https://mywebmaster.co.uk/images/whatsapp.png\" alt=\"whatsApp\" width=\"125px\" class=\"waimage\"> </a>\n</h3>\n        \n        </div>\n        </div>\n    </div>\n\n       <div class=\"container\">\n \n        <div class=\"row\">\n\n            <div class=\"col-xl-8 offset-xl-2\">\n\n                 <form id=\"contact-form\" class=\"mbr-form\" method=\"post\" action=\"contact.php\" role=\"form\">\n\n                    <div class=\"messages\"></div>\n\n                    <div class=\"controls\">\n                        <div class=\"row\">\n                            <div class=\"col-lg-6 mb-3\">\n                                <div class=\"form-group\">\n                                    <input id=\"form_name\" type=\"text\" name=\"name\" class=\"form-control\" placeholder=\"Your Name*\" required=\"required\" data-error=\"Firstname is required.\">\n                                    <div class=\"help-block with-errors\"></div>\n                                </div>\n                            </div>\n\n\n                            <div class=\"col-lg-6 mb-3\">\n                                \n                                   <div class=\"form-group\">\n                        \n                                    <input id=\"form_masjidname\" type=\"text\" name=\"masjidname\" class=\"form-control\" placeholder=\"Masjid Name\" required=\"required\" data-error=\"Masjid Name is required.\">\n                                    <div class=\"help-block with-errors\"></div>\n                              \n                                </div>\n                            </div>\n                        </div>\n                        <div class=\"row\">\n                            <div class=\"col-lg-6\">\n                                <div class=\"form-group\">\n                                    <input id=\"form_email\" type=\"email\" name=\"email\" class=\"form-control\" placeholder=\"Email *\" required=\"required\" data-error=\"Valid email is required.\">\n                                    <div class=\"help-block with-errors\"></div>\n                                </div>\n                            </div>\n                            <div class=\"col-lg-6\">\n                                <div class=\"form-group\">\n                                 \n                                    <input id=\"form_phone\" type=\"tel\" name=\"phone\" class=\"form-control\" placeholder=\"Phone\">\n                                    <div class=\"help-block with-errors\"></div>\n                                </div>\n                            </div>\n                        </div>\n                        <div class=\"form-group\">\n                          \n                            <textarea id=\"form_message\" name=\"message\" class=\"form-control\" placeholder=\"Message *\" rows=\"4\" required=\"required\" data-error=\"Please, leave us a message.\"></textarea>\n                            <div class=\"help-block with-errors\"></div>\n                        </div>\n\n                            <div class=\"form-group\">\n                            <div class=\"g-recaptcha\" data-sitekey=\"6LfgAh8TAAAAAD8bYwAgAsP5A20skDZnwa3kFuRh\" data-callback=\"verifyRecaptchaCallback\" data-expired-callback=\"expiredRecaptchaCallback\"></div>\n                            <input class=\"form-control d-none\" data-recaptcha=\"true\" required=\"\" data-error=\"Please complete the Captcha\">\n                            <div class=\"help-block with-errors\"></div>\n                        </div>\n                        \n                        \n                  \n          \n                         <input type=\"submit\" class=\"btn btn-success btn-send btnform\" value=\"Send message\">\n\n\n\n                        <p class=\"text-muted\">\n\n                        </p>\n\n                    </div>\n\n                </form>\n\n            </div>\n            <!-- /.8 -->\n\n        </div>\n        <!-- /.row-->\n\n    </div>\n    <!-- /.container-->\n\n\n\n</section>\n\n\n",
          "_customCSS": "/* Type valid CSS here */\n.btnform {\n    float: right;\n}\n.g-recaptcha {\n    float: left;\n}\n\n@media (max-width: 780px) {\n.g-recaptcha {\n    float: none;\n    margin: 0 auto;\n    display: table;\n    text-align: center;\n}\n.btnform {\n    float: none;\n    margin: 0 auto;\n    display: block;\n} \n}\n.form3 {\n    margin-top:90px;\n    background: #ffffff;\n    margin-bottom:80px;\n}\n.my-image {\n  width: 100%;\n}\n.mbr-section-subtitle {\n        color: #8d97ad;\n    margin-bottom:2em;\n}\n.mbr-section-title  {\n    font-weight:600;\n}\n.form-control {\n  color: grey !important;\n  border: none;\n  border-radius: 0 !important;\n  background-color: transparent;\n  padding: .5rem 0rem;\n  border-bottom: 1px solid grey;\n}",
